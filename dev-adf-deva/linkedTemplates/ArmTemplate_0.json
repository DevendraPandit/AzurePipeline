{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-adf-deva"
		},
		"LS_azuresqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_azuresqldb'"
		},
		"LS_azuresqldb_aaaprac_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_azuresqldb_aaaprac'"
		},
		"LS_gen2lake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_gen2lake'"
		},
		"LS_azuresqldb_aaaprac_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "devsqlserver12.database.windows.net"
		},
		"LS_azuresqldb_aaaprac_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "devsqldb"
		},
		"LS_azuresqldb_aaaprac_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "devendra"
		},
		"LS_gen2lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devstorage123321.dfs.core.windows.net/"
		},
		"LS_keyvault_aaaprac_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://devkeyvault1221.vault.azure.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/append_variable_activity')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Set variable1",
						"type": "SetVariable",
						"dependsOn": [],
						"policy": {
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"variableName": "persons_dtls",
							"value": [
								{
									"person_id": 101,
									"person_name": "srinivas"
								},
								{
									"person_id": 102,
									"person_name": "phani"
								}
							]
						}
					},
					{
						"name": "ForEach1",
						"type": "ForEach",
						"dependsOn": [
							{
								"activity": "Set variable1",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@variables('persons_dtls')",
								"type": "Expression"
							},
							"isSequential": true,
							"activities": [
								{
									"name": "Set variable2",
									"type": "SetVariable",
									"dependsOn": [],
									"policy": {
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"variableName": "person_name",
										"value": {
											"value": "@item().person_name",
											"type": "Expression"
										}
									}
								},
								{
									"name": "Append variable1",
									"type": "AppendVariable",
									"dependsOn": [
										{
											"activity": "Set variable2",
											"dependencyConditions": [
												"Succeeded"
											]
										}
									],
									"userProperties": [],
									"typeProperties": {
										"variableName": "names_lst",
										"value": {
											"value": "@variables('person_name')",
											"type": "Expression"
										}
									}
								}
							]
						}
					},
					{
						"name": "Set variable3",
						"type": "SetVariable",
						"dependsOn": [
							{
								"activity": "ForEach1",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"variableName": "msg",
							"value": {
								"value": "@concat('persons names are',variables('names_lst'))",
								"type": "Expression"
							}
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"variables": {
					"persons_dtls": {
						"type": "Array"
					},
					"person_name": {
						"type": "String"
					},
					"names_lst": {
						"type": "Array"
					},
					"msg": {
						"type": "String"
					}
				},
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/LS_azuresqldb')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('LS_azuresqldb_connectionString')]"
				}
