				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"server": "[parameters('LS_azuresqldb_aaaprac_properties_typeProperties_server')]",
					"database": "[parameters('LS_azuresqldb_aaaprac_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('LS_azuresqldb_aaaprac_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('LS_azuresqldb_aaaprac_password')]"
					}
				}
			},
			"dependsOn": []
