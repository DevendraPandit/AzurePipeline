{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-adf-deva"
		},
		"LS_azuresqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_azuresqldb'"
		},
		"LS_azuresqldb_aaaprac_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_azuresqldb_aaaprac'"
		},
		"LS_gen2lake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_gen2lake'"
		},
		"LS_azuredb_keyvault_aaaprac_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "devsqlserver12.database.windows.net"
		},
		"LS_azuredb_keyvault_aaaprac_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "devsqldb"
		},
		"LS_azuredb_keyvault_aaaprac_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "devendra"
		},
		"LS_azuresqldb_aaaprac_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "devsqlserver12.database.windows.net"
		},
		"LS_azuresqldb_aaaprac_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "devsqldb"
		},
		"LS_azuresqldb_aaaprac_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "devendra"
		},
		"LS_gen2lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devstorage123321.dfs.core.windows.net/"
		},
		"LS_keyvault_aaaprac_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://devkeyvault1221.vault.azure.net/"
		},
		"trigger_tumblingWindow_properties_pipeline_parameters_w_start_time": {
			"type": "string",
			"defaultValue": "@{trigger().outputs.windowStarTime}"
		},
		"trigger_tumblingWindow_properties_pipeline_parameters_w_end_time": {
			"type": "string",
			"defaultValue": "@{trigger().outputs.windowEndTime}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-adf-deva_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_azuresqldb_connectionString": {
						"value": "[parameters('LS_azuresqldb_connectionString')]"
					},
					"LS_azuresqldb_aaaprac_password": {
						"value": "[parameters('LS_azuresqldb_aaaprac_password')]"
					},
					"LS_gen2lake_accountKey": {
						"value": "[parameters('LS_gen2lake_accountKey')]"
					},
					"LS_azuresqldb_aaaprac_properties_typeProperties_server": {
						"value": "[parameters('LS_azuresqldb_aaaprac_properties_typeProperties_server')]"
					},
					"LS_azuresqldb_aaaprac_properties_typeProperties_database": {
						"value": "[parameters('LS_azuresqldb_aaaprac_properties_typeProperties_database')]"
					},
					"LS_azuresqldb_aaaprac_properties_typeProperties_userName": {
						"value": "[parameters('LS_azuresqldb_aaaprac_properties_typeProperties_userName')]"
					},
					"LS_gen2lake_properties_typeProperties_url": {
						"value": "[parameters('LS_gen2lake_properties_typeProperties_url')]"
					},
					"LS_keyvault_aaaprac_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_keyvault_aaaprac_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dev-adf-deva_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_azuredb_keyvault_aaaprac_properties_typeProperties_server": {
						"value": "[parameters('LS_azuredb_keyvault_aaaprac_properties_typeProperties_server')]"
					},
					"LS_azuredb_keyvault_aaaprac_properties_typeProperties_database": {
						"value": "[parameters('LS_azuredb_keyvault_aaaprac_properties_typeProperties_database')]"
					},
					"LS_azuredb_keyvault_aaaprac_properties_typeProperties_userName": {
						"value": "[parameters('LS_azuredb_keyvault_aaaprac_properties_typeProperties_userName')]"
					},
					"trigger_tumblingWindow_properties_pipeline_parameters_w_start_time": {
						"value": "[parameters('trigger_tumblingWindow_properties_pipeline_parameters_w_start_time')]"
					},
					"trigger_tumblingWindow_properties_pipeline_parameters_w_end_time": {
						"value": "[parameters('trigger_tumblingWindow_properties_pipeline_parameters_w_end_time')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dev-adf-deva_ArmTemplate_0')]"
			]
		},
		{
			"name": "dev-adf-deva_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dev-adf-deva_ArmTemplate_1')]"
			]
		},
		{
			"name": "dev-adf-deva_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dev-adf-deva_ArmTemplate_2')]"
			]
		}
	]
}